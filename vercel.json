{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/agents/chat",
      "destination": "/api-vercel/chat"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

