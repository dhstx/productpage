{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/agents/chat",
      "destination": "/api/agents/chat-function"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
